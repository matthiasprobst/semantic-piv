### Single-bit mask for a Flag (power of two).
piv:mask a owl:DatatypeProperty ;
  rdfs:domain piv:Flag ; rdfs:range xsd:unsignedInt ;
  skos:prefLabel "mask"@en ;
  skos:definition "Non-negative integer used for encoding. For bitwise schemes: powers of two; 0 is reserved for 'no flag'."@en .

### Human-readable meaning/label for a flag.
piv:meaning a owl:DatatypeProperty ;
  rdfs:domain piv:Flag ; rdfs:range xsd:string ;
  skos:prefLabel "meaning"@en ;
  skos:note "Prefer skos:prefLabel for the human-readable flag name; piv:meaning is kept for backward compatibility."@en ;
  skos:definition "Human-readable meaning of the atomic flag."@en .


piv:hasFlagValue a owl:DatatypeProperty ;
  rdfs:domain piv:FlagMapping ;
  rdfs:range xsd:unsignedInt ;
  skos:prefLabel "has flag value"@en ;
  skos:definition """
Concrete integer value that encodes one or more flags.
In a FlagMapping, it defines how numeric values map to flag definitions.
In a FlagValue, it represents the actual value stored with data.
"""@en .

###  https://matthiasprobst.github.io/pivmeta#timeValue
piv:timeValue rdf:type owl:DatatypeProperty ;
              rdfs:domain piv:TemporalVariable ;
              rdfs:range [ rdf:type rdfs:Datatype ;
                           owl:unionOf ( xsd:date
                                         xsd:dateTime
                                       )
                         ] ;
              skos:definition "Canonical timestamp value."@en ;
              skos:example "2024-07-25T13:45:30" ;
              skos:prefLabel "time value"@en .


###  https://matthiasprobst.github.io/ssno#standardName
ssno:standardName rdf:type owl:DatatypeProperty ;
                  rdfs:domain ssno:StandardName ;
                  rdfs:range xsd:string ;
                  skos:definition "Name for a standard name, e.g. \"x_velocity\". The construction of a standard name string is defined by the standard name table. E.g. no spaces are allowed and specific pre- and suffixes are allowed."@en ;
                  skos:example "x_derivative_of_y_velocity" ;
                  skos:prefLabel "standard name"@en .


###  https://matthiasprobst.github.io/ssno#withImageDeformation
ssno:withImageDeformation rdf:type owl:DatatypeProperty ;
                          rdfs:domain piv:InterrogationMethod ;
                          rdfs:range xsd:boolean ;
                          skos:definition "Flag, indicating whether the algorithm uses image deformation technique as described in Raffel et al. (2018) \"Particle Image Velocimetry: A Practical Guide\", Springer, https://doi.org/10.1007/978-3-319-68852-7."@en ;
                          skos:prefLabel "with image deformation"@en .


###  https://w3id.org/okn/o/sd#hasDocumentation
sd:hasDocumentation rdf:type owl:DatatypeProperty ;
                    rdfs:domain sd:Software ;
                    rdfs:range xsd:anyURI ;
                    skos:prefLabel "has documentation"@en .


#################################################################
#    Classes
#################################################################

### An indivisible flag condition (atomic bit) like "active" or "filtered".
piv:Flag a owl:Class ;
  rdfs:subClassOf skos:Concept ;
  skos:prefLabel "Flag"@en ;
  skos:definition """
An elementary flag concept representing one minimal, indivisible condition
that may be assigned to data points in a PIV dataset (e.g., “active”, “masked”, “filtered”).
Each Flag corresponds to a single power-of-two mask value that can be
combined with others by bitwise OR operations to form composite flag values.
"""@en ;
  skos:note "Use piv:mask for the numeric bit value and piv:meaning (or skos:prefLabel) for the human-readable label."@en .

piv:Flag rdfs:subClassOf
  [ a owl:Restriction ; owl:onProperty piv:mask ; owl:cardinality "1"^^xsd:nonNegativeInteger ],
  [ a owl:Restriction ; owl:onProperty piv:meaning ; owl:cardinality "1"^^xsd:nonNegativeInteger ] .


### A collection defining which atoms are valid for a dataset and how to interpret values.
piv:FlagScheme a owl:Class ;
  skos:prefLabel "flag scheme"@en ;
  skos:definition """
A scheme defining the set of valid flag atoms and the interpretation rules for flag
values in a dataset. A scheme declares whether values are mutually exclusive
(enumerated) or bitwise-combinable.
"""@en .


### Explicit mapping entry that binds a literal integer value to a specific.
piv:FlagMapping a owl:Class ;
  skos:prefLabel "flag mapping"@en ;
  skos:definition "Associates a concrete integer value with a piv:Flag within a scheme."@en .
### Scheme type taxonomy to make the semantics machine-readable.
piv:FlagSchemeType a owl:Class ;
  skos:prefLabel "flag scheme type"@en ;
  skos:definition "Describes how flag values are interpreted (bitwise vs enumerated)."@en .

piv:BitwiseFlagScheme a owl:Class ;
  rdfs:subClassOf piv:FlagSchemeType ;
  skos:prefLabel "bitwise flag scheme"@en ;
  skos:definition "Flag values can be combined using bitwise OR; atoms are recovered via bitwise AND with each piv:mask."@en .

piv:EnumeratedFlagScheme a owl:Class ;
  rdfs:subClassOf piv:FlagSchemeType ;
  skos:prefLabel "enumerated flag scheme"@en ;
  skos:definition "Each flag value represents a mutually exclusive state; combinations are not used."@en .


